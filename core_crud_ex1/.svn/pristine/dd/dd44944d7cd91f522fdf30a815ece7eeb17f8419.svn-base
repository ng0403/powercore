@CHARSET "UTF-8";

.a_dboard:link {
	color: blue;
	text-decoration: none;
}

.a_dboard:visited {
	color: blue;
	text-decoration: none;
}

.a_dboard:hover {
	color: blue;
	text-decoration: underline;
}

.a_dboard:active {
	color: blue;
	text-decoration: none;
}

#dataSearchBtn{
	width: 70px;
}

#dataPeople{
	width: 200px;
}

#dataDepartment{
	width: 160px;	
}

#dataSearch{
	width: 400px;
}

#dataDivision{
	width: 130px;
}

th {
	width: 130px;
	text-align: left;
	padding-right: 2px;
}

#SearchTable{
	width: 1000px;
}

form {
	display: inline;
}

#searchbody{
	width: 100%;
}

.people_btn{
	 font-size:10pt;
  	 background-color:#eaeaea;
 	 color:#353535;
 	 border:1px solid #C0C4CB;
 	 padding:5px 5px;
 	 border-radius:5px 5px 5px 5px;
 	 width: auto;
 	 margin-right: 5px;
 	 cursor: pointer;
 	 vertical-align: middle;
}

#searchboardbtn{
	 font-size:10pt;
  	 background-color:#eaeaea;
 	 color:#353535;
 	 border:1px solid #C0C4CB;
 	 padding:5px 5px;
 	 border-radius:5px 5px 5px 5px;
 	 width: auto;
 	 margin-right: 5px;
 	 cursor: pointer;
 	 vertical-align: middle;
 	 margin-top: 10px;
}

.int_ad {
	position: relative;
	width: 30%;
	height: 24px;
	border: 1px solid #bebebe;
	font-size: 15px;
	color: #333;
	border-radius: 5px 5px 5px 5px;
	margin-right: 10px;
	margin-top: 9px;
}

.inttop2 {
	position: relative;
	width: 120px;
	height: 24px;
	border: 1px solid #bebebe;
	font-size: 0.9em;
	color: #333;
	z-index: 10;
	border-radius: 5px 5px 5px 5px;
	margin-right: 10px;
	margin-top: 7px;
}

html{
	font-family: 'Nanum Gothic', Dotum, "돋움", Gulim, "굴림", Verdana, Arial, "Trebuchet MS";
	border-collapse: collapse;
	font-size: 13px;
}

#title .caption{
  	color:#484848;
  	font-weight:bold;
  	border-bottom: 1px solid #dddddd;
  	padding-bottom: 10px;
  }

#dboardbody{
	margin-left: 2%;
	margin-right: 2%;
}

#dataselectbox{
	background-color: #ffffff;
	color: #5F5F5F;
	border: 1px solid #C0C4CB;
	padding: 4px 4px;
	border-radius: 5px 5px 5px 5px;
	width: 100px;
	margin-top: 7px;
}

#dataselectbox1{
	background-color: #ffffff;
	color: #5F5F5F;
	border: 1px solid #C0C4CB;
	padding: 4px 4px;
	border-radius: 5px 5px 5px 5px;
	width: 90px;
	margin-top: 7px;
}

#dataselectbox2{
	background-color: #ffffff;
	color: #5F5F5F;
	border: 1px solid #C0C4CB;
	padding: 4px 4px;
	border-radius: 5px 5px 5px 5px;
	width: 120px;
	margin-top: 7px;
}

#dboardtable{
	width:100%;
	margin-top: 10px;
	border-top: 2px solid #C0C4CB;
	border-bottom: 2px solid #C0C4CB;
	border-collapse: collapse;
}

#dboardtable thead tr{
	border-bottom: 2px solid #C0C4CB;
	background-color: #F2F2F2;
	font-weight: bold;
}

#dboardtable td{
	height:25px;
}

#dboardtable tbody td{
	color: black;
	border-bottom: 1px solid #DDDDDD;
	padding:1.5px;
}

#dboardtable tbody tr:HOVER{
	background-color: #ebe7e7;
}

#directbtndiv{
	margin-top: 20px;
}

#searchdboardinput{
	position: relative;
	width: 160px;
	height: 24px;
	border: 1px solid #bebebe;
	color: #333;
	border-radius: 5px 5px 5px 5px;
	margin-top: 7px;
	float: right;
}

.searchbtn{
	margin-top: 7px;
	margin-left: 7px;
	float: right;
	display: inline-block;
	padding: 4px 9px;
	font-weight: normal;
	line-height: 1.428571429;
	text-align: center;
	white-space: nowrap;
	vertical-align: middle;
	cursor: pointer;
	border: 1px solid transparent;
	border-radius: 4px;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	-o-user-select: none;
	user-select: none;
	color: #353535;
	background-color: #EAEAEA;
	border-color: #BDBDBD;
}

#searchdboardinput[placeholder]{
	color: gray;
	text-align:center;
}

#btndiv{
	float: left;
}

#btndivStore{
	float: right;
}

.btn {
	display: inline-block;
	padding: 4px 9px;
	margin-bottom: 0;
	font-weight: normal;
	line-height: 1.428571429;
	text-align: center;
	white-space: nowrap;
	vertical-align: middle;
	cursor: pointer;
	border: 1px solid transparent;
	border-radius: 4px;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	-o-user-select: none;
	user-select: none;
	color: #353535;
	background-color: #EAEAEA;
	border-color: #BDBDBD;
}

.btn-default:hover{
	color: #ffffff;
	background-color: #6B6B6B;
	border-color: #BDBDBD;
}

#numbtndiv{
	text-align:center;
}

.numbtn{
	border: #bebebe;
	border-radius: 5px 5px 5px 5px;
}