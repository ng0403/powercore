@CHARSET "UTF-8";

html{
	font-family: 'Nanum Gothic', Dotum, "돋움", Gulim, "굴림", Verdana, Arial, "Trebuchet MS";
	border-collapse: collapse;
    border-spacing: 0;
    font-size:.9em;
    font-weight:700;
}

#title .caption{
  	color:#484848;
  	font-weight:bold;
  	border-bottom: 1px solid #dddddd;
  	padding-bottom: 10px;
  }
 
.bs-example{
	border-bottom: 1px solid #dddddd;
	padding-bottom: 15px;
}

#contPopTable{
	margin-left: 15px;
}

#tbody tr{
	height: 40px;
}

#contPopTable #tbody tr th{
	padding-right:10px;
	text-align: right;
	width:75px;
}
  
#contPopTable tbody > tr > th {	
	line-height: 1.4;
	width : 60px;
	vertical-align: middle;
}

#contPopTable tbody > tr > td {
  line-height: 1.4;
  vertical-align: middle;
  width: 150px;
  padding:3px;
}

.auth_bt_position{
	float:right;
	margin-top: 10px;
}

.int{
	position:relative;
	width:180px;
	height:24px;
	border:1px solid #bebebe;
	color:#232323;
	border-radius: 5px 5px 5px 5px;
	margin-right: 10px;
}

.int_name{
	position:relative;
	width:130px;
	height:24px;
	border:1px solid #bebebe;
	color:#232323;
	border-radius: 5px 5px 5px 5px;
	margin-right: 10px;
}

.int_email{
	position:relative;
	width:100px;
	height:24px;
	border:1px solid #bebebe;
	font-size:15px;
	color:#232323;
	border-radius: 5px 5px 5px 5px;
	margin-right: 10px;
}

.int_act{
	position:relative;
	width:120px;
	height:24px;
	border:1px solid #bebebe;
	color:#232323;
	border-radius: 5px 5px 5px 5px;
	margin-right: 10px;
}

.memo{
	border:1px solid #bebebe;
	border-radius: 5px 5px 5px 5px;
	width:233px;
}

.cont_bt_position {
	float: right;
	padding-right: 9px;
	margin-top:10px;
}

.cust_cont_btn{
  	 background-color:#eaeaea;
 	 color:#353535;
 	 border:1px solid #C0C4CB;
 	 padding:5px 5px;
 	 border-radius:5px 5px 5px 5px;
 	 width: auto;
 	 cursor: pointer;
 	 vertical-align: middle;
}

.cust_cont_btn:hover{
  	 background-color:#ABABAB;
 	 color:white;
 	 border:1px solid #C0C4CB;
 	 padding:5px 5px;
 	 border-radius:5px 5px 5px 5px;
 	 width: auto;
 	 cursor: pointer;
 	 vertical-align: middle;
}