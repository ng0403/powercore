@CHARSET "UTF-8";
/*기본값*/
html {
	font-family: 'Nanum Gothic', Dotum, "돋움", Gulim, "굴림", Verdana, Arial,
		"Trebuchet MS";
	border-collapse: collapse;
	font-size: 13px;
}

select {
	margin-left: 10px;
	background-color: #ffffff;
	color: #5F5F5F;
	border: 1px solid #C0C4CB;
	padding: 5px 5px;
	border-radius: 5px 5px 5px 5px;
	width: 120px;
/* 	margin-right: 30px; */
}

/*계약 리스트*/
/*계약 타이틀*/
#title .caption {
	color: #484848;
	font-weight: bold;
	border-bottom: 1px solid #dddddd;
	padding-bottom: 10px;
}

/*연락처 검색부분*/
.search_div {
	margin-top: 10px;
	margin-right: 10px;
	color: #484848;
	font-weight: bold;
	margin-bottom: 10px;
}

.tel_label_list {
/* 	margin-right: 10px; */
	margin-left: 7px;
}

.tel_search {
	position: relative;
	width: 8%;
	height: 24px;
	border: 1px solid #bebebe;
	color: #232323;
	border-radius: 5px 5px 5px 5px;
	margin-right: 10px;
	padding-left: 5px;
}

.tel_date_search {
	position: relative;
	width: 8%;
	height: 24px;
	border: 1px solid #bebebe;
	color: #232323;
	border-radius: 5px 5px 5px 5px;
}

/*계약 리스트 테이블*/
#tableline {
	padding-top: 10px;
	width: 100%;
}

#tableline2 {
	float: right;
	padding-top: 19px;
	width: 70%;
}

#goaltable {
	border: 2px solid #DDD;
	width: 100%;
	border-right: thin;
	height: 180px;
	border-collapse: collapse;
}

#goaltable th {
	width: 20px;
	text-align: center;
}

#goaltable thead {
	text-align: center;
}

#goaltable tr th, #goaltable tr td {
	border: 1px solid #DDD;
	height: 10px;
}

#goaltable thead th, #goaltable thead td {
	background-color: #F2F2F2;
	font-weight: bold;
}

#goaltable tbody tr, #goaltable tbody tr td {
	text-align: center;
}

/*영업활동 페이징 부분*/
#pagingDiv {
	color: #232323;
	float: left;
	margin-top: 10px;
	font-weight: bold;
}

.cont_page_txt {
	position: relative;
	width: 22px;
	height: 20px;
	border: 1px solid #bebebe;
	color: #232323;
	border-radius: 5px 5px 5px 5px;
	text-align: center;
}

/*영업활동 리스트 버튼*/
.cont_list_bt_div {
	float: right;
	margin-top: 10px;
}

.cont_btn {
	float: right;
	background-color: #eaeaea;
	color: #353535;
	border: 1px solid #C0C4CB;
	padding: 5px 5px;
	border-radius: 5px 5px 5px 5px;
	width: auto;
	margin-right: 5px;
	cursor: pointer;
	vertical-align: middle;
}

.cont_btn:hover {
	background-color: #ABABAB;
	color: white;
	border: 1px solid #C0C4CB;
	padding: 5px 5px;
	border-radius: 5px 5px 5px 5px;
	width: auto;
	cursor: pointer;
	vertical-align: middle;
}

.cntClick:link {
	text-decoration: none;
}

.cntClick:visited {
	text-decoration: none;
}

.cntClick:hover {
	text-decoration: underline;
}

.cntClick:active {
	text-decoration: none;
}