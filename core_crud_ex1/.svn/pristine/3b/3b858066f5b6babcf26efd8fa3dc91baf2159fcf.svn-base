@CHARSET "EUC-KR";
html{
	font-family: 'Nanum Gothic', Dotum, "µ¸¿ò", Gulim, "±¼¸²", Verdana, Arial, "Trebuchet MS";
	font-size: 13px;
}

#tableline{
	overflow-x: hidden;
	overflow-x: auto;
}

#goaltable{
	border: 2px solid #DDD;
	width: 100%;
	height:80px;
	border-collapse: collapse;
}

#goaltable th{
	width: 5%;
	text-align: center;
}

#goaltable thead{
	text-align: center;
}

#goaltable tr,
#goaltable tr td{
	border: 1px solid #DDD;
}

#goaltable thead th,
#goaltable thead td {
	background-color: #F2F2F2;
	font-weight: bold;
	height:10px;
	padding: 5px 5px;
}

#goaltable tbody tr,
#goaltable tbody tr td {
	text-align: center;
	padding-right: 2px;
	padding-top: 2px;
	padding-bottom: 2px;
}

.normal td:nth-of-type(3){
	text-align: right;
}
.homeOpptAtag:link {
	color: blue;
	text-decoration: none;
}

.homeOpptAtag:visited {
	color: blue;
	text-decoration: none;
}

.homeOpptAtag:hover {
	color: blue;
	text-decoration: underline;
}

.homeOpptAtag:active {
	color: blue;
	text-decoration: none;
}
