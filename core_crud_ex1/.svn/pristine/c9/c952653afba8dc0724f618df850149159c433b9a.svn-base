	border: 1px solid #C0C4CB;
	padding: 5px 5px;
	border-radius: 5px 5px 5px 5px;
	width: 60px;
	margin-right: 5px;
}

#goal_button_modify:hover:enabled {
	background-color: #6b6b6b;
	color: #ffffff;
}

#goal_button_modify_position {
	margin-left: 100px;
	margin-top: 10px;
	float: right;
}

#goal_button_cancel {
	float: right;
	font-size: 12px;
	background-color: #eaeaea;
	color: #353535;
	border: 1px solid #C0C4CB;
	padding: 5px 5px;
