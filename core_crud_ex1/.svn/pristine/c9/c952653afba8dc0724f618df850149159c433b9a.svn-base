	margin-left: 100px;
	margin-top: 10px;
	float: right;
}

#goaltable thead th, #goaltable thead td {
	background-color: #F2F2F2;
	font-weight: bold;
}

#goaltable tbody {
	text-align: center;
}

#goaltable thead th, #goaltable thead td {
	background-color: #F2F2F2;
	font-weight: bold;
}

#secondtable thead th, #secondtable thead td {
	background-color: #F2F2F2;
	font-weight: bold;
}

.goalparenttr td{
	background-color: #FFFFC6;
	text-align: right;
