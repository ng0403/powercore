html{
	font-family: 'Nanum Gothic', Dotum, "돋움", Gulim, "굴림", Verdana, Arial, "Trebuchet MS";
	border-collapse: collapse;
	font-size:13px;
}
input[type=text]{
	font-size: 1em;
}
.search_div{
	font-weight: bold;
	color:#484848;
	margin-top: 7px;
	margin-bottom: 7px;
	padding-right: 10px;
}

#scust_nm{
	margin-right: 0px;
}

#oppt_lev_label{
	margin-left: 40px;
}
.tel_search {
	position: relative;
	width: 8%;
	height: 24px;
	border: 1px solid #bebebe;
	color: #333;
	border-radius: 5px 5px 5px 5px;
	margin-right: 40px;
}
.btn-success-tel {
	display: inline-block;
	padding: 4px 9px;
	margin-bottom: 0;
	font-weight: normal;
	font-size:.9em;
	line-height: 1.428571429;
	text-align: center;
	white-space: nowrap;
	vertical-align: middle;
	cursor: pointer;
	border: 1px solid transparent;
	border-radius: 4px;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	-o-user-select: none;
	user-select: none;
	color: #353535;
	background-color: #EAEAEA;
	border-color: #BDBDBD;
}
.btn-success-tel:hover, .btn-success-tel:focus, .btn-success-tel:active,
	.btn-success-tel.active, .open .dropdown-toggle.btn-success-tel {
	color: #ffffff;
	background-color: #6B6B6B;
	border-color: #BDBDBD;
}
#title .caption{
  	color:#484848;
  	font-weight:bold;
  	border-bottom: 1px solid #dddddd;
  	padding-bottom: 10px;
  }
  
#contact_button_position{
	float: right;
	margin-top: 5px;
}


.oppt_nm_class{
	cursor: pointer;
}


#goaltable {
	border-collapse: collapse;
}

