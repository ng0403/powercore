#ccPageInput {
	width: 22px;
	height: 18px;
	text-decoration: none;
	text-align: center;
	position: relative;
	border: 1px solid #bebebe;
	color: #333;
	border-radius: 5px 5px 5px 5px;
}



.custcomp_btn{
  	 background-color:#eaeaea;
 	 color:#353535;
 	 border:1px solid #C0C4CB;
 	 padding:5px 5px;
 	 border-radius:5px 5px 5px 5px;
 	 width: 45px;
 	 margin-right: 5px;
 	 cursor: pointer;
 	 vertical-align: middle;
}

#pageSpace {
	float: left;
	margin-top: 5px;
	padding-right: 10px;
}

