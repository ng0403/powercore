@charset "utf-8";

/* reset */
html,body,form,div,p,h1,h2,h3,h4,h5,h6,dl,dt,dd,ul,ol,li,fieldset,th,td,button{margin:0;padding:0;border:0;font-size:100%; font:inherit; font-weight:normal;}

html, body {
	height: 100%;
	font-family: 'Nanum Gothic', Dotum, "돋움", Gulim, "굴림", Verdana, Arial, "Trebuchet MS";
	}
body{-webkit-text-size-adjust: none;}

article, aside, details, figcaption, figure, footer, header, hgroup, nav, section {display: block;}

input, textarea, select, button{font:inherit;vertical-align:middle;}
button{background:none; border: none; margin: 0; padding: 0;width: auto; overflow: visible; cursor: pointer;}

input[type="button"], input[type="text"], input[type="password"], input[type="image"], #loginBtn, textarea {-webkit-appearance:none; border-radius:0;}
input[type="checkbox"]{-webkit-appearance:checkbox;margin:0;}
input[type="radio"] {-webkit-appearance:radio;padding:0;margin:0;vertical-align:middle;}

#loginBtn {
	width:100px; 
	height:80px;
	border:1px solid #C0C4CB;
	border-radius:5px 5px 5px 5px;
	cursor: pointer;
	font-family: 'Nanum Gothic', Dotum, "돋움", Gulim, "굴림", Verdana, Arial, "Trebuchet MS";
	font-weight: bold;
	}
	
textarea {resize:none;}
select {margin:0;}
input[type="text"], input[type="password"] {padding:0 0 0 12px;}

table{width:100%;border-collapse:collapse;border-spacing:0;}
table caption, legend {position:absolute;left:-9999em;text-indent:-9999em;font-size:0;line-height:0;visibility:hidden;}
ol, ul{list-style:none;}
img, fieldset, iframe{border:none; vertical-align:middle;}
address, cite, code, dfn, em, var, th, strong{font-style:normal; font-weight:normal;}

button,
a {color: #707070; text-decoration:none;}
button:hover,
a:link,a:visited {color:#666;text-decoration:none;}
a:hover {color:#666;text-decoration:none;}
a:active {color:#666;text-decoration:none;}
button:focus,a:focus, input:focus, textarea:focus, button:focus {outline:1px dotted #4b4b4b;}


.clearfix:after{content:""; display:block; clear:both;}
* html .clearfix{ zoom: 1; } /* IE6 */
*:first-child+html .clearfix { zoom: 1; } /* IE7 */

hr {display: none;}

body, input, textarea, button, select, table {font:12px '돋움',Dotum,sans-serif;color:#666;}

.hide {position: absolute; left: -9999em; text-indent:-9999em;font-size:0;line-height:0;z-index:-1;}

/*로그인*/
body {background:#29334b;}
#wrap {position:absolute;left:50%;top:50%;width:1054px;height:644px;margin:-322px 0 0 -527px;background:#fff; }
.loginWrap {padding:0 0 70px 280px;}
.h1Logo {padding:30px 0 32px 11px;}
.h1Ttx {padding:0 0 0 12px;height:128px;color:#8fa8cc;line-height:19px;}

.loginWrap input[type="text"], .loginWrap input[type="password"] {
	width:262px;
	height:37px;
	background:white;
	line-height:37px;
	border:1px solid #C0C4CB;
	border-radius:5px 5px 5px 5px;
	font-family: 'Nanum Gothic', Dotum, "돋움", Gulim, "굴림", Verdana, Arial, "Trebuchet MS";
	}
	
.loginWrap input:focus {background:url("../images/bg_input_on.gif") no-repeat;}
.loginWrap fieldset {overflow:hidden; padding:5px 0 70px 75px;}
.loginWrap fieldset div {float:left;}
.loginWrap fieldset p {position:relative;padding:0 12px 6px 0;}
.loginWrap fieldset label {display:block;position:absolute;left:16px;top:9px;width:107px;height:22px;}
.loginWrap fieldset label.on {left:-9999px;}
