.loginChk {padding:16px 0 2px 2px;}
.loginChk * {vertical-align:middle;}
.loginChk label {display:inline-block;width:39px;height:10px;margin:0 5px 0 5px;background:url("../images/txt_save.gif") no-repeat;}
.loginChk .selectBox {display:inline-block;position:relative;zoom:1;*display:inline;margin-top:2px;}
.loginBtn {padding:1px 0 5px 270px;}
.loginBtn * {vertical-align:middle;text-align:right;}
.selectBox button {display:inline-block;width:91px;padding-left:10px;height:25px;background:url('../images/btn_language_s.gif') no-repeat;color:#737781;text-align:left;font-weight:bold;}
.selectBox ul {display:none;position:absolute;left:0;top:25px;width:89px;background:#fff;border:1px solid #c4c8d0;}
.selectBox ul.show {display:block;}
.selectBox ul li a {display:block;height:22px;padding-left:9px;line-height:22px;border-top:1px solid #c4c8d0;}
.selectBox ul li a:hover, .selectBox ul li a:focus {background:#f9fafb;}
.selectBox ul li:first-child a {border-top:0;}
.loginWrap .info {padding:0 0 0 7px;background:url('../images/bl_info.gif') no-repeat left 2px;}

#footer {position:relative; text-align: center;  margin-top: -640px;}
.footerLogo {position:relative;left:0px;top:20px;}
.footMenu {position:relative;left: 0px;
           top: 600px;
           width: 711px;
           padding: 21px 0px 100px 14px;
           border-top: 1px solid #d2d7de;
           margin : 0 auto;}
.footMenu .list {overflow:hidden;font-size:11px;}
.footMenu .list li {padding:0 8px 0 9px;background:white;}
.footMenu .list li:first-child {padding-left:0;background:none;}
.footMenu .list a:hover {color:#000;}
.footMenu .address {padding:13px 0 6px;}
#center{width: 100px;}

