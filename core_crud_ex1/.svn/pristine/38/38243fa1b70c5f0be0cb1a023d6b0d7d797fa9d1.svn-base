@CHARSET "UTF-8";
/*기본값*/
html{
	font-family: 'Nanum Gothic', Dotum, "돋움", Gulim, "굴림", Verdana, Arial, "Trebuchet MS";
	font-size:13px;
}

select{
	 margin-left: 10px;
  	 background-color:#ffffff;
 	 color:#5F5F5F;
 	 border:1px solid #C0C4CB;
 	 padding:5px 5px;
 	 border-radius:5px 5px 5px 5px;
 	 width: auto;
 	 margin-left: 0px;
}

.callClick:link {
	text-decoration: none;
}

.callClick:visited {
	text-decoration: none;
}

.callClick:hover {
	text-decoration: underline;
}

.callClick:active {
	text-decoration: none;
}

