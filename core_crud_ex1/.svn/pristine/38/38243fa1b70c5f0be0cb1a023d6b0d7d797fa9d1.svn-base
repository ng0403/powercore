/*연락처 리스트 테이블*/    
#tableline{
	padding-top: 10px;
	width: 100%;	
	
}

#tableline2{
	float:right;
	padding-top: 19px;
	width: 70%; 
}

#goaltable{
	border: 2px solid #DDD;
	width: 100%;
	border-right:thin;
	height:180px;
	border-collapse: collapse;
}

#goaltable th{
	width: 20px;
	text-align: center;
}

#goaltable thead{
	text-align: center;
}

#goaltable tr,
#goaltable tr td{
	border: 1px solid #DDD;
/* 	height:10px; */
}

#goaltable thead th,
#goaltable thead td {
	background-color: #F2F2F2;
	font-weight: bold;
}

#goaltable thead th, #goaltable tbody td{
	border: 1px solid #DDD;
	text-align: center;
	height: 25px;
	padding: 3px;
}

#goaltable tbody tr,
#goaltable tbody tr td {
	text-align: center;
}

/*영업활동 페이징 부분*/
