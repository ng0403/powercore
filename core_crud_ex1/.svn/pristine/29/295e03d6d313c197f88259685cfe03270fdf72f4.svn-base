package com.crm.cp.standard.data_board.vo;

import java.sql.Date;

import org.springframework.stereotype.Component;

@Component
public class AttachVO {
	
	String attach_id;
	String data_board_id;
	String file_root;
	Date reg_d;
	int attach_idx;
	
	String filename;
	
	public AttachVO(){}
	
	public AttachVO(String attach_id, String data_board_id, String file_root,
			Date reg_d) {
		super();
		this.attach_id = attach_id;
		this.data_board_id = data_board_id;
		this.file_root = file_root;
		this.reg_d = reg_d;
	}
	
	public String getAttach_id() {
		return attach_id;
	}
	public void setAttach_id(String attach_id) {
		this.attach_id = attach_id;
	}
	public String getData_board_id() {
		return data_board_id;
	}
	public void setData_board_id(String data_board_id) {
		this.data_board_id = data_board_id;
	}
	public String getFile_root() {
		return file_root;
	}
	public void setFile_root(String file_root) {
		this.file_root = file_root;
	}
	public Date getReg_d() {
		return reg_d;
	}
	public void setReg_d(Date reg_d) {
		this.reg_d = reg_d;
	}
	public int getAttach_idx() {
		return attach_idx;
	}

	public void setAttach_idx(int attach_idx) {
		this.attach_idx = attach_idx;
	}
	public String getFilename() {
		return filename;
	}

	public void setFilename(String filename) {
		this.filename = filename;
	}
	
}
